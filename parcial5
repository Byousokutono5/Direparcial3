#!/bin/bash

echo "Content-type: text/html; charset=UTF-8"
echo ""
echo "<html><body>"
echo "<h1> Parcial 2 - Seminario - Francisco Santos </h1>"
echo "<h2> Fecha</h2>"
echo "<pre>$(date)</pre>"
echo "<h2> Hostname</h2>"
echo "<pre>$(hostname)</pre>"
echo "<h2> IP Address </h2>"
echo "<pre>$(ipconfig)</pre>"
echo "<h2> Route</h2>"
echo "<pre>$(route PRINT)</pre>"
echo "<h2> SS</h2>"
echo "<pre>$(netstat -a)</pre>"
echo "<h2> NMAP </h2>"
echo "<pre>$(nmap -v -p 22 www.nmap.org)</pre>"
echo "<h2> Uname </h2>"
echo "<pre>$(uname -rms)</pre>"
echo "<h2> Uptime </h2>"
echo "<pre>$(net statistics server )</pre>"
echo "<h2> Procesador</h2>"
echo "<pre>$(systeminfo)</pre>"
echo "</body></html>
